<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tencent.wxcloudrun.dao.LocationMapper">

    <resultMap id="locationMapper" type="com.tencent.wxcloudrun.model.Location">
        <id property="id" column="id"/>
        <result property="community_id" column="community_id" javaType="java.lang.String"/>
        <result property="tag1" column="tag1" javaType="java.lang.String"/>
        <result property="tag2" column="tag2" javaType="java.lang.String"/>
    </resultMap>

    <select id="getAllLocation" resultMap="locationMapper" parameterType="java.lang.String">
        SELECT *
        FROM location_list
        WHERE community_id = #{community_id}
    </select>

</mapper>
